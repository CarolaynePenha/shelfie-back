{
  "name": "shelfie-back",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "npm i && tsc && prisma migrate deploy",
    "start": "npx node dist/prisma/seed.js && node dist/src/server.js",
    "dev": "nodemon ./src/server.ts",
    "dev:test": "dotenv -e .env.test nodemon src/server.ts",
    "db-seed":"dotenv -e .env prisma db seed ",
    "db-seed:test":"dotenv -e .env.test prisma db seed ",
    "test": "dotenv -e .env.test prisma migrate dev && npm run db-seed:test && cross-env NODE_OPTIONS=--experimental-vm-modules dotenv -e .env.test jest -- -i",
    "test:unit": "dotenv -e .env.test cross-env NODE_OPTIONS=--experimental-vm-modules jest -- -i --coverage --testPathPattern=tests/unit"
  },
  "prisma": {
    "seed": "ts-node dist/prisma/seed.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@prisma/client": "^5.10.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prisma": "^5.10.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  }
}
